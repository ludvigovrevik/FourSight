        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Text fx:id="p1Text" layoutX="264.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Player 1" />
            <TextField fx:id="player1" layoutX="265.0" layoutY="74.0" prefHeight="26.0" prefWidth="180.0" />
            <Text fx:id="p2Text" layoutX="263.0" layoutY="133.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Player 2" />
            <TextField fx:id="player2" layoutX="265.0" layoutY="137.0" prefHeight="26.0" prefWidth="180.0" />
            <Button fx:id="Ok" layoutX="263.0" layoutY="186.0" mnemonicParsing="false" onAction="#enableGame" text="Start spill!" />
